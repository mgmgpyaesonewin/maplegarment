{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "max-len": ["off"],
      "no-tabs": ["off"],
      "no-confusing-arrow": ["off"],
      "jsx-a11y/anchor-is-valid": [ "off", {
        "components": [ "Link" ]
      }]
    }
}